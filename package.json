{
  "name": "electro",
  "productName": "Electro",
  "version": "0.0.6",
  "description": "Electro DJ",
  "license": "MIT",
  "repository": "jspear29/electro",
  "author": {
    "name": "Joseph Spear"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "start": "gulp run",
    "build": "gulp build && cp package.json source/ && electron-packager build/ electro --out=dist --platform=darwin --arch=x64 --icon=source/assets/icons/electro.icn  --prune --overwrite",
    "package": "cd dist/electro-darwin-x64/ && tar -cvzf electro-$npm_package_version.tar.gz electro.app && mv electro-$npm_package_version.tar.gz ../../ && cd ../../ "
  },
  "files": [
    "index.js",
    "index.html",
    "index.css"
  ],
  "keywords": [
    "electron-app"
  ],
  "dependencies": {
    "array-buffer-concat": "^1.0.1",
    "configstore": "^2.0.0",
    "debug-menu": "^0.4.0",
    "electron": "^1.0.0",
    "flux": "^2.1.1",
    "font-awesome": "^4.6.3",
    "jquery": "^3.1.1",
    "jquery-knob": "^1.2.11",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-mdl": "^1.9.0",
    "react-slider": "^0.6.1",
    "youtube-audio-stream": "0.0.16"
  },
  "devDependencies": {
    "electron-packager": "^7.3.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.1",
    "gulp-cli": "^1.1.0",
    "gulp-concat": "^2.6.0",
    "gulp-rename": "^1.2.2",
